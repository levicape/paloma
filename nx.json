{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "parallel": 1,
  "cacheDirectory": "/tmp/npm-global/nx-cache",
  "targetDefaults": {
    "build": {
      "cache": true,
      "outputs": [
        "{projectRoot}/module",
        "{projectRoot}/commonjs",
        "{projectRoot}/build-http",
        "{projectRoot}/build-staticwww",
        "{projectRoot}/build-unknown",
        "{projectRoot}/gen"
      ]
    }
  }
}